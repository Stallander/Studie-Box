#include <iostream>
#include <string>
#include <vector>

using namespace std;


string antwoord;
string keuze;
int score=0;
bool oefenen=true;
std::vector<string> fouteWoordenEN;
std::vector<string> fouteWoordenFR;
std::vector<string> fouteWoordenNL;

void buildUp31(){
int j=0;
string buildUp31EN[25]={"pond", "hose", "to mow", "lawn", "lawnmower", "path", "gravel", "gate", "fance", "board", "carpenter", "nail", "to water", "tap", "tool", "shed", "orchard", "greenhouse",
"to dig", "gap", "to gather", "to weed", "to sweep", "broom", "bunch"};
string buildUp31NL[25]={"vijver", "slang", "maaien", "gazon", "maaimachine", "pad", "grind", "hek; poort", "hek; schutting", "plank", "timmerman", "spijker", "water geven", "kraan", "gereedschap",
"schuurtje", "boomgaard", "kas", "graven", "opening", "verzamelen", "wieden", "vegen", "bezem", "bos"};
    for(int i=0;i<=24;i++){
        cout << "Vertaal het volgende woord correct naar het Engels:" << endl;
        cout << buildUp31NL[i] << endl;
        std::getline(cin,antwoord);
        if(i==0)
            std::getline(cin,antwoord);
        if(antwoord==buildUp31EN[i]){
            cout<<"Goed"<<endl;
            score++;
        }else{
            cout<<"Fout, het moest zijn '"<<buildUp31EN[i]<<"'."<< endl;
            fouteWoordenEN.push_back(buildUp31EN[i]);
            fouteWoordenNL.push_back(buildUp31NL[i]);
            j++;
        }
    }
cout<<"Je score is "<<score<<"/25"<<endl;
cout<<"Wil je de woorden die je fout had nog een keer oefenen?('ja' of 'nee')"<<endl;
cin >> keuze;
if(keuze=="ja"){
    score=0;
    for(int i=0;i<j;i++){
        cout << "Vertaal het volgende woord correct naar het Engels:" << endl;
        cout << fouteWoordenNL[i] << endl;
        std::getline(cin,antwoord);
        if(i==0)
            std::getline(cin,antwoord);
        if(antwoord==fouteWoordenEN[i]){
            cout<<"Goed"<<endl;
            score++;
        }else{
            cout<<"Fout, het moest zijn '"<<fouteWoordenEN[i]<<"'."<< endl;
        }
    }
cout<<"Van de woorden die je eerst fout had heb je er nu "<<score<<" van de "<<j<<" goed."<<endl;
}
else
    return;
}
