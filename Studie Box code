        }
    }
cout<<"Van de woorden die je eerst fout had heb je er nu "<<score<<" van de "<<j<<" goed."<<endl;
}
else
    return;
}
void Ch3A(){
int j=0;
string Ch3AFR[20]={"le spectale", "assister á", "chanter", "la chanson", "la voix", "l'origine", "la nouvelle", "toute l'année", "dernier, dernière", "pas mal", "tu as vu", "tu connais",
"la rencontre", "comment", "trop (de)", "trouver", "oublier", "donner", "remplir", "allô"};
string Ch3ANL[20]={"de voorstelling", "bijwonen", "zingen", "het liedje", "de stem", "de oorsprong", "het nieuwtje", "het hele jaar", "laatste, vorige", "niet slecht", "jij hebt gezien", "jij kent",
"de ontmoeting", "hoe", "te, te veel", "vinden", "vergeten", "geven", "invullen", "hallo(aan de telefoon)"};
    for(int i=0;i<=19;i++){
        cout << "Vertaal het volgende woord correct naar het Engels:" << endl;
        cout << Ch3ANL[i] << endl;
        std::getline(cin,antwoord);
        if(i==0)
            std::getline(cin,antwoord);
        if(antwoord==Ch3AFR[i]){
            cout<<"Goed"<<endl;
            score++;
        }else{
            cout<<"Fout, het moest zijn '"<<Ch3AFR[i]<<"'."<< endl;
            fouteWoordenFR.push_back(Ch3AFR[i]);
            fouteWoordenNL.push_back(Ch3ANL[i]);
            j++;
        }
    }
cout<<"Je score is "<<score<<"/27"<<endl;
cout<<"Wil je de woorden die je fout had nog een keer oefenen?('ja' of 'nee')"<<endl;
cin >> keuze;
if(keuze=="ja"){
    score=0;
    for(int i=0;i<j;i++){
        cout << "Vertaal het volgende woord correct naar het Engels:" << endl;
        cout << fouteWoordenNL[i] << endl;
        std::getline(cin,antwoord);
        if(i==0)
            std::getline(cin,antwoord);
        if(antwoord==fouteWoordenFR[i]){
            cout<<"Goed"<<endl;
            score++;
        }else{
            cout<<"Fout, het moest zijn '"<<fouteWoordenFR[i]<<"'."<< endl;
        }
    }
cout<<"Van de woorden die je eerst fout had heb je er nu "<<score<<" van de "<<j<<" goed."<<endl;
}
else
    return;
}
