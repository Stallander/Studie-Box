    cout<<"'Ch3A'(Chapitre 3 lijst A)"<<endl;
    cout<<"Om te stoppen, typ dan in 'exit', of klik op het kruisje rechts bovenin."<<endl;
    cout<<"Voor bugs kun je altijd contact opnemen per mail(22450@student.csgpm.nl) of via WhatsApp."<<endl;
    cin>>keuze;
    if(keuze=="BU31"||keuze=="bu31")
        buildUp31();
    if(keuze=="BU32"||keuze=="bu32")
        buildUp32();
    if(keuze=="BU33"||keuze=="bu33")
        buildUp33();
    if(keuze=="BU34"||keuze=="bu34")
        buildUp34();
    if(keuze=="BU35"||keuze=="bu35")
        buildUp35();
    if(keuze=="BU36"||keuze=="bu36")
        buildUp36();
    if(keuze=="BU37"||keuze=="bu37")
        buildUp37();
    if(keuze=="BU38"||keuze=="bu38")
        buildUp38();
    if(keuze=="BU39"||keuze=="bu39")
        buildUp39();
    if(keuze=="BU40"||keuze=="bu40")
        buildUp40();
    if(keuze=="Ch3A"||keuze=="CH3A"||keuze=="Ch3a"||keuze=="CH3a"||keuze=="ch3a")
        Ch3A();
